{
  // https://nuxt.com/docs/guide/concepts/typescript
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "exclude": [
    "./.nuxt/tsconfig.json",
    "node_modules", 
    ".nuxt", 
    "dist"
  ]
  // "extends": 
}
